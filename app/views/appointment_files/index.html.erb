 <div class="right_col" role="main" style="min-height: 1063px;">
  <div class="">
    <div >
    <h1>Appointment Files</h1>

    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Desc</th>
          <th>Document</th>
          <th>Appointment</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @appointment_files.each do |appointment_file| %>
          <tr>
            <td><%= appointment_file.name %></td>
            <td><%= appointment_file.desc %></td>
            <td><%= appointment_file.document %></td>
            <td><%= appointment_file.appointment %></td>
            <td><%= link_to 'Show', appointment_file %></td>
            <td><%= link_to 'Edit', edit_appointment_file_path(appointment_file) %></td>
            <td><%= link_to 'Destroy', appointment_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>
    
    <div class="clearfix"></div>
  </div>
</div>

